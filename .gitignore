//
void setup() {
 size(1200,900); 
 background(255);
 noStroke();
}

void draw() {
  translate(width/2,height/2);
  scale(0.1);
  for (int i = 0; i<19; i++) {
   fill(255,25);
   rect(-1000,-1000,2000,2000);
   fill(0);
   kutular();
  rotate(-10*PI/180); 
  scale(1.15);
 
  }
 

}

void kutular() {
  rect(200-width/2,350-height/2,150,150);
  rect(450-width/2,350-height/2,300,150);
  rect(850-width/2,350-height/2,150,150);
}

